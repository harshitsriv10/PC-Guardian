<Page x:Class="PCGaurdianV1.userPortal"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PCGaurdianV1"
      mc:Ignorable="d" 
      d:DesignHeight="350" d:DesignWidth="500"
      Title="userPortal">

    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center"  >
            <TextBlock Name="info" HorizontalAlignment="Left" Height="57" Margin="0,0,0,0" TextAlignment="Center" FontSize="24" Foreground="White" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="409"/>
            <Button Name="logout" Content="Logout" HorizontalAlignment="Center" Height="67" Margin="0,0,0,0" VerticalAlignment="Top" Width="181" Click="logout_Click"/>
        </StackPanel>
    </Grid>
</Page>
